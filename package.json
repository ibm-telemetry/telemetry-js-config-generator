{
  "name": "@ibm/telemetry-js-config-generator",
  "description": "Automated script to generate IBM Telemetry config files",
  "version": "2.0.1",
  "license": "Apache-2.0",
  "author": "IBM",
  "keywords": [
    "ibm",
    "telemetry",
    "opentelemetry"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "index.js",
  "bin": {
    "ibmtelemetry-config": "dist/index.js"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "scripts/lint",
    "lint:fix": "scripts/lint_and_fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "tmp-promise": "^3.0.3",
    "yaml": "^2.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@types/node": "^20.16.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsdoc": "^48.11.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.10",
    "typescript": "^5.5.4"
  }
}
